    <section class="second clearfix" style="background-image:url(/images22/bg3.png);margin-top:4%;margin-left:.4%">
      
    <div style="margin-left:.3%">
        @foreach(var uu in   ViewBag.EditPic12 )
        {
            if(uu.Videos!=null)
            {
            <article class="video">
        <figure>
        <a class="fancybox fancybox.iframe"  href="@uu.Videos"><img class="videoThumb" style="width:270px;height:270px;margin-left:2.5%" src="~/Gallary/unnamed.png"></a>
        </figure><br /><br /><br /><br /><br>
        <h2 class="videoTitle" style="text-align:center">@uu.Video_Title</h2>
                
      </article>
        }
        }
